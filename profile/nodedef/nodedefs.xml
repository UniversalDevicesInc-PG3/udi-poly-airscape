<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller, this must mach the id in your Controller class-->
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
			<st id="ST" editor="bool" />
		</sts>
        <cmds>
            <sends />
            <accepts>
              <cmd id="QUERY" />
              <cmd id="DISCOVER" />
              <cmd id="UPDATE_PROFILE" />
            </accepts>
        </cmds>
    </nodeDef>
	<!-- airscape node -->
    <nodeDef id="airscape" nls="airs">
        <editors />
        <sts>
            <st id="ST"  editor="bool"  />
            <st id="GV1" editor="SPEED" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
                <cmd id="SET_SPEED">
                  <p id="" editor="SPEED" init="GV1" />
                </cmd>
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
